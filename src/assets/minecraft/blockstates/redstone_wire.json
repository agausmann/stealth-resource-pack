{
    "multipart": [
        {
            "when": { "OR": [
                { "north": "side|up" },
                { "north": "none", "east": "none", "south": "side|up", "west": "none" },
                { "north": "none", "east": "none", "south": "none", "west": "none" }
            ]},
            "apply": { "model": "block/redstone_dust_side" }
        },
        {
            "when": { "OR": [
                { "east": "side|up" },
                { "north": "none", "east": "none", "south": "none", "west": "side|up" },
                { "north": "none", "east": "none", "south": "none", "west": "none" }
            ]},
            "apply": { "model": "block/redstone_dust_side", "y": 90 }
        },
        {
            "when": { "OR": [
                { "south": "side|up" },
                { "north": "side|up", "east": "none", "south": "none", "west": "none" },
                { "north": "none", "east": "none", "south": "none", "west": "none" }
            ]},
            "apply": { "model": "block/redstone_dust_side", "y": 180 }
        },
        {
            "when": { "OR": [
                { "west": "side|up" },
                { "north": "none", "east": "side|up", "south": "none", "west": "none" },
                { "north": "none", "east": "none", "south": "none", "west": "none" }
            ]},
            "apply": { "model": "block/redstone_dust_side", "y": 270 }
        },

        { "when": { "north": "up" }, "apply": { "model": "block/redstone_dust_up" } },
        { "when": { "east": "up" }, "apply": { "model": "block/redstone_dust_up", "y": 90 } },
        { "when": { "south": "up" }, "apply": { "model": "block/redstone_dust_up", "y": 180 } },
        { "when": { "west": "up" }, "apply": { "model": "block/redstone_dust_up", "y": 270 } },

        { "when": { "power": 0 }, "apply": { "model": "block/redstone_dust_power_0" } },
        { "when": { "power": 1 }, "apply": { "model": "block/redstone_dust_power_1" } },
        { "when": { "power": 2 }, "apply": { "model": "block/redstone_dust_power_2" } },
        { "when": { "power": 3 }, "apply": { "model": "block/redstone_dust_power_3" } },
        { "when": { "power": 4 }, "apply": { "model": "block/redstone_dust_power_4" } },
        { "when": { "power": 5 }, "apply": { "model": "block/redstone_dust_power_5" } },
        { "when": { "power": 6 }, "apply": { "model": "block/redstone_dust_power_6" } },
        { "when": { "power": 7 }, "apply": { "model": "block/redstone_dust_power_7" } },
        { "when": { "power": 8 }, "apply": { "model": "block/redstone_dust_power_8" } },
        { "when": { "power": 9 }, "apply": { "model": "block/redstone_dust_power_9" } },
        { "when": { "power": 10 }, "apply": { "model": "block/redstone_dust_power_10" } },
        { "when": { "power": 11 }, "apply": { "model": "block/redstone_dust_power_11" } },
        { "when": { "power": 12 }, "apply": { "model": "block/redstone_dust_power_12" } },
        { "when": { "power": 13 }, "apply": { "model": "block/redstone_dust_power_13" } },
        { "when": { "power": 14 }, "apply": { "model": "block/redstone_dust_power_14" } },
        { "when": { "power": 15 }, "apply": { "model": "block/redstone_dust_power_15" } }
    ]
}
